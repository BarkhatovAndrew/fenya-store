{
  "name": "fenya-store",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msmarielos/fenya-store.git"
  },
  "license": "ISC",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "npm run build:client && cd server && npm install && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all && node ./src/app.js",
    "build:client": "npm run build --workspace=client",
    "start:client": "npm run start --workspace=client",
    "start:server": "npm run dev --workspace=server",
    "format": "npm run format --workspaces --if-present"
  }
}
